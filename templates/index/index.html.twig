{% extends 'base.html.twig' %}

{% block title %}Товары{% endblock %}

{% block body %}
<div class="example-wrapper">
    <h1>Товары</h1>
    <ul>
        {% for item in products %}
            <li><a href="{{ path('product', {'name': item.name}) }}">{{ item.name }}</a></li>
        {% endfor %}
    </ul>
</div>
{% endblock %}
